    <div class="container">
        <div class="row">
            <div class="col">
                <table class="table table-bordered">
                    <thead>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">name</th>
                        <th scope="col">mobile</th>
                        <th scope="col">email</th>
                        <th scope="col">birthday</th>
                        <th scope="col">address</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(let r of rows ){ %>  
                      <tr>
                        
                        <td><%= r.sid %></td>
                        <td><%= r.name %></td>
                        <td><%= r.mobile %></td>
                        <td><%= r.email %></td>
                        <td><%= toDatetString(r.birthday) %></td>
                        <td><%= r.address %></td>
                        
                      </tr>
                      <% } %>
                    </tbody>
                  </table>
            </div>
        </div>

        <div class="row">
            <div class="col">
                
            </div>
        </div>
    </div>
